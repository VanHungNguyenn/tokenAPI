<!DOCTYPE html>
<html lang="en">
	<head>
		<meta charset="UTF-8" />
		<meta http-equiv="X-UA-Compatible" content="IE=edge" />
		<meta name="viewport" content="width=device-width, initial-scale=1.0" />
		<title>Document</title>
	</head>
	<body>
		<h1>All Tokens</h1>
		<!-- render all token with table -->
		<table>
            <thead>
                <tr>
                    <th>UID</th>
                    <th>Token</th>
                    <th>Live</th>
                    <th>Created At</th>
                    <th>Updated At</th>
                </tr>
            </thead>
            <tbody>
                <!-- loop through all tokens -->
                <% _.each(tokens, function(token) { %>
                    <tr>
                        <td><%= token.uid %></td>
                        <td><%= token.token %></td>
                        <td><%= token.live %></td>
                        <td><%= token.createdAt %></td>
                        <td><%= token.updatedAt %></td>
                    </tr>
            </tbody>
		<!-- render pagination with total, page, limit -->
		<ul>
			<% for (var i = 0; i < pages.length; i++) { %>
			<li><%= pages[i] %></li>
			<% } %>
		</ul>
	</body>
</html>
